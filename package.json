{
  "name": "@svc/markdown-notes",
  "version": "0.6.0",
  "description": "Markdown Notes using local storage",
  "main": "src/index.tsx",
  "scripts": {
    "build:dev": "webpack -p --mode development",
    "build:prod": "webpack -p --mode production",
    "release": "semantic-release --debug",
    "start": "PORT=8082 webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamthamc/MarkdownNotes.git"
  },
  "keywords": [
    "Javascript",
    "Markdown",
    "Notes",
    "Notes"
  ],
  "author": "Chaitanya Kumar Kamatham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamthamc/MarkdownNotes/issues"
  },
  "homepage": "https://github.com/kamthamc/MarkdownNotes#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@material-ui/core": "^4.0.0-beta.1",
    "@material-ui/icons": "^4.0.0-beta.0",
    "@material-ui/styles": "^4.0.0-beta.1",
    "file-saver": "^2.0.2",
    "immer": "^3.1.1",
    "marked": "^0.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl-universal": "^1.16.1",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "latest",
    "redux": "^4.0.1",
    "redux-devtools-extension": "latest",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.2.0",
    "typeface-roboto": "0.0.54",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.7",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@types/marked": "^0.6.5",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-router-dom": "^4.3.3",
    "@types/styled-components": "^4.1.14",
    "@types/uuid": "^3.4.4",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "semantic-release": "^15.13.13",
    "ts-loader": "^5.4.5",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            "dist/*.{js,css,html}"
          ]
        }
      ]
    ],
    "ci": true
  }
}
